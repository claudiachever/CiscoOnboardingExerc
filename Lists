  adding = x.extend([5,6,7,8,9,10])
 x.extend(range(5,11))
 print(x)

 for number in range(5,11):
     x.append(number)

 print(x)


 3.

 for number in x:
     if number % 2 == 0:
         print('Numarul {} este par'.format(number))
     else:
         print('Numarul {} este impar'.format(number))

  for number in x:
      if number % 2 == 0:
          print('Numarul %d este par' %(number))
      else:
          print('Numarul %d este impar' %(number))


