 else:
     print("Error: lungimea este aceeasi")

 myfamily = {
   "child1" : {
     "name" : "Emil",
     "year" : 2004
   },
   "child2" : {
     "name" : "Tobias",
     "year" : 2007
   },
   "child3" : {
     "name" : "Linus",
     "year" : 2011
   }
 }


 print(myfamily.pop("child1", None))
 
  print (myfamily)

 del myfamily["child1"]
 print(myfamily)



def count_small(numbers):
  total = 0
  for n in numbers:
    if n < 10:
      total = total + 1
  return total

lotto = [4, 8, 15, 16, 23, 42]
small = count_small(lotto)
print (small)



def fizz_count(x):
  count = 0
  for item in x:
    if item == "fizz":
      count = count + 1
  return count

print(fizz_count(["fizz","cat","fizz"]))
